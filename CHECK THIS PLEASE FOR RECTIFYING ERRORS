        $("#AddVideoLink").click(function () {
            ShowPopUpVideo('popupVideo', 'popupbgVideo');
        });
        function ShowPopUpVideo(PopUpWindow, PopUpBackGround) {
            close = "closeimagereview";
            PopUpBackGround = "." + PopUpBackGround;
            PopUpWindow = "." + PopUpWindow;
            close = "." + close;
            $(PopUpBackGround).css("width", document.width);
            $(PopUpBackGround).css("height", document.height);
            $(PopUpBackGround).css("top", 0);
            $(PopUpBackGround).css("left", 0);
            $(PopUpBackGround).css("z-index", 10000);
            $(PopUpBackGround).css("background", "#aaaaaa");
            $(PopUpBackGround).css("opacity", ".30");
            $(PopUpBackGround).css("filter", "Alpha(Opacity=30)");


            $("#imgCancelfw").css('display', 'none');
            $("#imgokfw").css('display', 'none');
            //$("#Loding").dialog("close");
            $(PopUpWindow).css("width", 600);
            $(PopUpWindow).css("height", 450);
            $(PopUpWindow).css("top", (self.innerHeight - 425) / 2);
            $(PopUpWindow).css("left", (self.innerWidth - 650) / 2);
            $(PopUpWindow).css("z-index", 10020);
            $(PopUpWindow).css("background-color", "black");
            $(PopUpWindow).css("opacity", "0.6");
            $(close).css("top", ((self.innerHeight - 400) / 2) - 10);
            $(close).css("left", ((self.innerWidth - 600) / 2) + 600 - 20);
            $(close).css("z-index", 10040);
            $('#txtEmail').val(user.getEmailId());
            $(PopUpBackGround).fadeIn(function () {
                $(PopUpWindow).fadeIn();
                $(close).fadeIn();
            });

            $('.closeimagereview').bind('click', function () {
                $(PopUpWindow).fadeOut();
                $(close).fadeOut();

                $(PopUpBackGround).fadeOut();
            });

            $('#CloseVideo').bind('click', function () {
                $(PopUpWindow).fadeOut();
                $(close).fadeOut();

                $(PopUpBackGround).fadeOut();

            });
        };
        function getViewDetail(Id) {
            window.location.href = "VideoDetail.aspx?VID=" + Id;
        }
        $('#allvlogs').click(function () {
            window.location.href = "YouVideo.aspx";
        });
        function removeVideo(Id) {
            $.ajax({
                type: "POST",
                async: false,
                data: getParameterJson("Id", Id),
                url: "WebService.asmx/" + "Delete_YourVideo",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        alert('Delete Video Successfully.');
                        $('#youvlogs').click();
                    }
                }
            });
        }
        function InserHomeVideo(Id) {
            $.ajax({
                type: "POST",
                async: false,
                data: getParameterJson("Id", Id),
                url: "WebService.asmx/" + "Insert_HomeVideo",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        alert('Insert Video Successfully.');
                       // $('#youvlogs').click();
                    }
                }
            });
        
        }
