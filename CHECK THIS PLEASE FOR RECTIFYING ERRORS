    <script type="text/javascript">
        $(document).ready(function () {

            var user;
            IsUserLoggedIn();
        });
        function IsUserLoggedIn(callback) {
            var strHtml = '';
            $.ajax({
                type: "POST",
                async: false,
                url: "WebService.asmx/" + "IsUserLoggedIn",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        user = new User(result.Id, result.UserName, result.EmailId);
                        $("#gmaillogin").hide();
                        $('#divbutton').show();
                        $("#divLogout").show();
                        strHtml = '<span style=" margin-left: 15px; margin-right: 8%; margin-bottom: 0%;"> USER-X :  ' + user.getUserName() + '</span>';
                        if (user.getEmailId() == 'vicky278711@gmail.com') {
                            $("#btnblog").show();
                            $(".deleteblog").show();
                        }
                        $("#divUserName").append(strHtml)
                        if (callback) callback();
                    }
                    
                }
            });
        }
    
    </script>
    <script type="text/javascript">
        function OpenGoogleLoginPopup() {


            var url = "https://accounts.google.com/o/oauth2/auth?";
            url += "scope=https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/userinfo.email&";
            url += "state=%2Fprofile&"
            url += "redirect_uri=http://www.xtech7.com/gmail.aspx&"
            url += "response_type=token&"
            url += "client_id=439331344006.apps.googleusercontent.com&";
            url += "approval_prompt=auto";
            window.location = url;
        }

        $("#divLogout").click(function () {
            $.ajax({
                type: "POST",
                async: false,
                url: "WebService.asmx/" + "LogOut",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        $("#gmaillogin").show();
                        $('#divbutton').hide();
                        $("#divLogout").hide();
                        $("#divUserName").html('');
                        user = null;
                    }
                }
            });
        });
        $('#AddBlog').click(function () {
            window.location.href = "AddBlog.aspx";

        });
        $('#btnxmit').click(function () {
            $.ajax({
                type: "POST",
                async: false,
                data: getParameterJson("EmailId", $('#txtEmail').val(), "VideoLink", $('#txtVideo').val(), "Title", $('#txtTitle').val(), "Description", $('#txtDescription').val()),
                url: "WebService.asmx/" + "Insert_YouVideo",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        alert("Video save Successfully.")
                    }
                }
            });

        });
        $('#SnClose').click(function () {
            $('.popupbgVideo').hide();
            $('.popupVideo').hide();
        });
    </script>
