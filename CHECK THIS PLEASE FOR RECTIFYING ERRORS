        var Id = 0;
        $(document).ready(function () {
           //var user = null;

           
            var strHTML = '';

            $.ajax({
                type: "POST",
                async: false,
                data: getParameterJson("Id", Id),
                url: "WebService.asmx/" + "Get_AllYouVideo",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        strHTML = '';
                        if (result.length > 4) {
                            var count = 0;
                            count = result.length - 1;
                        }
                        else {
                            count = result.length;
                        }
                        for (var i = 0; i < count; i++) {
                            if (i == 0 || i == 2) {
                                strHTML += '<div style=" float: left; margin-right: 5%; ">';
                            }
                            else {
                                strHTML += '<div style="float: left;">';
                            }

                            var src = '' + result[i].VideoLink + '';
                            strHTML += '<div><h3><span>' + result[i].Title.substring(0, 32) + '</span><h3></div>';
                            strHTML += '<div> <iframe src="' + src + '" title="application/x-shockwave-flash"  width="400" height="300"></iframe></div>';
                            
                            strHTML += '<div> Add By ' + result[i].EmailId + '</div>';
                            strHTML += '<div style="margin-left:35%;"><a style="cursor:pointer;" href="' + SEOnamegenerate(result[i].Title) + '" ><img src="images/Expand.gif" style=" width: 120px; height: 32px;"/></a></div></div>';
                            Id = result[i].Id;
                            if (i == count - 1) {

                                $("#youvideo").append(strHTML);
                            }
                        }
                        if (result.length >= 5) {
                            $('#divpage').show();
                        }
                    }
                }
            });
        });

        $('#youvlogs').click(function () {
            $("#youvideo").html('');
            $('#divpage').hide();
            Id = 0;
            var strHTML = '';
            var EmailId = user.getEmailId();
            $.ajax({
                type: "POST",
                async: false,
                data: getParameterJson("EmailId", EmailId, "Id", Id),
                url: "WebService.asmx/" + "Get_YourVideo",
                datatype: "json",
                contentType: 'application/json; charset=utf-8',
                success: function (response) {
                    result = response.d.result;
                    if (result == null)
                        return;
                    else {
                        strHTML = '';
                        if (result.length > 4) {
                            var count = 0;
                            count = result.length - 1;
                        }
                        else {
                            count = result.length;
                        }
                        for (var i = 0; i < count; i++) {
                            if (i == 0 || i == 2) {

                                strHTML += '<div style=" float: left; margin-right: 5%; ">';

                            }
                            else {
                                strHTML += '<div style="float: left;">';
                            }

                            var src = '' + result[i].VideoLink + '';
                            strHTML += '<div><h3><span>' + result[i].Title.substring(0, 32) + '</span><h3></div>';
                            strHTML += '<div> <iframe src="' + src + '" title="application/x-shockwave-flash"  width="400" height="300"></iframe></div>';
                            if (EmailId == "vicky278711@gmail.com") {
                                strHTML += '<div><a onclick="InserHomeVideo("'+ src +'")" style="cursor:pointer; color:red; float:right;">Set Home</a></div>';
                            }
                            strHTML += '<div><span> Add By ' + result[i].EmailId + '</span><a onclick="removeVideo(' + result[i].Id + ')" style="cursor:pointer; color:red; float:right;">Delete</a></div>';
                            strHTML += '<div style="margin-left:35%;"><a style="cursor:pointer;" href="' + SEOnamegenerate(result[i].Title) + '"); ></a><img src="images/Expand.gif" style=" width: 120px; height: 32px;"/></div></div>';
                            Id = result[i].Id;

                            if (i == count - 1) {
                                $("#youvideo").append(strHTML);
                            }
                        }
                        if (result.length >= 5) {
                            $('#divpageyour').show();
                        }
                    }
                }
            });


        });
